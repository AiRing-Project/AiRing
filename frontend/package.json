{
  "name": "AiRing",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "prettier": "prettier --write .",
    "lint:fix": "npm run lint -- --fix && npm run prettier",
    "android:reinstall": "adb uninstall com.airing.frontend.debug && npm run android",
    "android:clean-build": "cd android && ./gradlew clean build && cd ..",
    "android:apply-env-change": "npx react-native start --reset-cache",
    "android:assemble-apk": "cd android && ./gradlew assembleRelease && cd ..",
    "android:assemble-aab": "cd android && ./gradlew bundleRelease && cd ..",
    "start": "react-native start",
    "test": "jest",
    "prepare": "cd .. && husky frontend/.husky"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@notifee/react-native": "^9.1.8",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-navigation/bottom-tabs": "^7.3.13",
    "@react-navigation/native": "^7.1.9",
    "@react-navigation/native-stack": "^7.3.13",
    "@react-navigation/stack": "^7.3.2",
    "axios": "^1.9.0",
    "jwt-decode": "^4.0.0",
    "react": "19.0.0",
    "react-hook-form": "^7.56.4",
    "react-native": "0.79.2",
    "react-native-calendars": "^1.1312.1",
    "react-native-gifted-charts": "^1.4.61",
    "react-native-image-picker": "^8.2.1",
    "react-native-keychain": "^10.0.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-month-year-picker": "^1.9.0",
    "react-native-permissions": "^5.4.1",
    "react-native-reanimated": "^3.18.0",
    "react-native-safe-area-context": "^5.4.1",
    "react-native-screens": "^4.10.0",
    "react-native-svg": "^15.12.0",
    "react-native-vector-icons": "^10.2.0",
    "yup": "^1.6.1",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.2",
    "@react-native/eslint-config": "0.79.2",
    "@react-native/metro-config": "0.79.2",
    "@react-native/typescript-config": "0.79.2",
    "@types/jest": "^29.5.13",
    "@types/jwt-decode": "^2.2.1",
    "@types/react": "^19.0.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^19.0.0",
    "eslint": "^8.19.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "jest": "^29.6.3",
    "lint-staged": "^16.1.0",
    "prettier": "2.8.8",
    "react-native-dotenv": "^3.4.11",
    "react-native-svg-transformer": "^1.5.1",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
